{
  "name": "server",
  "version": "1.0.0",
  "description": "easy_send server",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "David Ohana RSW",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "enum": "^2.5.0",
    "express": "^4.13.3",
    "lodash": "4.17.21",
    "morgan": "^1.8.0",
    "needle": "^2.4.0",
    "request": "^2.88.0",
    "resource-router-middleware": "^0.6.0",
    "xml2json": "^0.11.2"
  },
  "devDependencies": {
    "@types/express": "4.17.0",
    "babel-cli": "^6.9.0",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.6",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  }
}
